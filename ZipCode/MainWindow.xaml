<Window x:Class="ZipCode.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZipCode"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <TextBlock>Enter Valid Zip Code(US or Canada):</TextBlock>
        <TextBox x:Name="uxZip" MaxLength="10" CharacterCasing="Upper" TextChanged="uxZip_TextChanged"></TextBox>
        <Button x:Name="uxSubmit" IsEnabled="False">Submit</Button>
    </StackPanel>
</Window>
